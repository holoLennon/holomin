<ion-tabs class="tabs-icon-top tabs-stable tabs-color-positive {{hideTabs}}" ng-class="{'tabs-item-hide': $root.hideTabs}">

  <ion-tab title="大数据" icon-off="ion-c-shouye-outline" icon-on="ion-c-shouye-copy" ng-click ="nowtabs('A')" >
    <ion-nav-view name="tab_c-a"></ion-nav-view>
  </ion-tab>

  <ion-tab title="云药房" icon-off="ion-c-yunyaofang-outline" icon-on="ion-c-yunyaofang-copy" ng-click ="nowtabs('B')" >
    <ion-nav-view name="tab_c-b"></ion-nav-view>
  </ion-tab>

  <ion-tab title="药店" icon-off="ion-c-yaodian-outline" icon-on="ion-c-yaodian-copy" ng-click ="nowtabs('C')" >
    <ion-nav-view name="tab_c-c"></ion-nav-view>
  </ion-tab>

  <ion-tab title="我的" icon-off="ion-a-person-outline" icon-on="ion-a-person-copy" ng-click ="nowtabs('D')">
    <ion-nav-view name="tab_c-d"></ion-nav-view>
  </ion-tab>
</ion-tabs>
