appctrl.controller("ProductListCtrl",function(t,e,r,i,n,o,c,a,l,u,s,d,g,f,p,P){d.debug("enter ProductList ctrl");t.page=_.clone(_Page);t.list=[];t.$on("$ionicView.beforeEnter",function(){d.debug("ProductList ctrl beforeEnter")});t.$on("$ionicView.afterEnter",function(){d.debug("ProductList ctrl afterEnter");if(ctrlReinitMap.get("ProductListCtrl")){ctrlReinitMap.remove("ProductListCtrl");d.debug("ProductList ctrl afterEnter init");t.init()}});t.init=function(){d.debug("ProductList ctrl init ");t.page.where=g.get("ProductList"+"QueryWhere");t.page.pageNo=1;t.page.hasNextPage=false;t.list=[];t.first()};t.doRefresh=function(){t.init()};t.addList=function(e){angular.forEach(e,function(e){t.list.push(e)});if(e&&e.length<t.page.pageSize){t.page.hasNextPage=false}else{t.page.hasNextPage=true}if(t.list.length<1){P.alertm("药品正在上架中...")}};t.first=function(){p.first(null,t.page).then(function(e){d.debug("ProductList ctrl query then");t.addList(e);t.$broadcast("scroll.refreshComplete")})};t.more=function(){d.debug("ProductList ctrl more=========");if(!t.page.hasNextPage){return}p.more(t.page).then(function(e){d.debug("ProductList ctrl more then");t.addList(e);t.$broadcast("scroll.infiniteScrollComplete")})};t.init();ctrlReinitMap.remove("ProductListCtrl")});