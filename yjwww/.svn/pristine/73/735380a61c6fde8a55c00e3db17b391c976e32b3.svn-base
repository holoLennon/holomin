<!--Suggestion，意见建议详细信息-->
<ion-view view-title='意见建议' cache-view='false'>
  <ion-nav-buttons side="right">
		<h4></h4>
  </ion-nav-buttons>
  <ion-content class="has-header" >
	<ul class="list" ng-if="obj">
		<li class="item">序号ID:{{obj.id}}</li>
		<li class="item">创建时间:{{obj.gmtCreateString}}</li>
		<li class="item">消费者会员:{{obj.memberIdString}}</li>
		<li class="item">处理员:{{obj.cadminId}}</li>
		<li class="item">内容:{{obj.detail}}</li>
		<li class="item">手机号:{{obj.mobile}}</li>
		<li class="item">备注说明:{{obj.cmemo}}</li>
	</ul>
    <ion-spinner class="topic-spinner ng-hide" ng-show="!obj" ng-style="{'top':getHalfHeight()+'px'}"></ion-spinner>
  </ion-content>
</ion-view>
