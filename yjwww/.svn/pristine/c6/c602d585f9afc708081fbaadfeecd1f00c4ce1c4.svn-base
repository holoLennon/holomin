appctrl.controller("ProductListSearchCtrl",function(t,e,r,i,n,c,o,a,l,u,s,d,f,g,p,h){f.debug("enter ProductList ctrl");t.kw=u.kw;t.page=_.clone(_Page);t.list=[];t.$on("$ionicView.beforeEnter",function(){f.debug("ProductList ctrl beforeEnter")});t.$on("$ionicView.afterEnter",function(){f.debug("ProductList ctrl afterEnter");if(ctrlReinitMap.get("ProductListSearchCtrl")){ctrlReinitMap.remove("ProductListSearchCtrl");f.debug("ProductList ctrl afterEnter init");t.init()}});t.init=function(){f.debug("ProductList ctrl init ");t.page.where="title like '%"+t.kw+"%'";t.page.pageNo=1;t.page.hasNextPage=false;t.list=[];t.first()};t.doRefresh=function(){t.init()};t.addList=function(e){angular.forEach(e,function(e){t.list.push(e)});if(e&&e.length<t.page.pageSize){t.page.hasNextPage=false}else{t.page.hasNextPage=true}};t.first=function(){h.first(null,t.page).then(function(e){f.debug("ProductList ctrl query then");t.addList(e);t.$broadcast("scroll.refreshComplete")})};t.more=function(){f.debug("ProductList ctrl more=========");if(!t.page.hasNextPage){return}h.more(t.page).then(function(e){f.debug("ProductList ctrl more then");t.addList(e);t.$broadcast("scroll.infiniteScrollComplete")})};t.init();ctrlReinitMap.remove("ProductListSearchCtrl")});