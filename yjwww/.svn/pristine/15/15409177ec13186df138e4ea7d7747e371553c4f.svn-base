<?xml version='1.0' encoding='utf-8'?>
<widget id="com.zmax.yijian" version="1.2.0" ios-CFBundleVersion="18" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>yijian</name>
    <description>
        yijian
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <chcp>
        <config-file url="http://yijian.zjjnyd.com/yijianwwwupdate/chcp.json"/>
    </chcp>
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
	<preference name="BackupWebStorage" value="local" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <!-- iOS 8.0+ -->
        <!-- iPhone 6 Plus  -->
        <icon src="res/ios/appicon/icon-60@3x.png" width="180" height="180" />
        <!-- iOS 7.0+ -->
        <!-- iPhone / iPod Touch  -->
        
        <icon src="res/ios/appicon/icon-60@2x.png" width="120" height="120" />
        <!-- iPad -->
        <icon src="res/ios/appicon/icon-76.png" width="76" height="76" />
        <icon src="res/ios/appicon/icon-76@2x.png" width="152" height="152" />
        <!-- iOS 6.1 -->
        <!-- Spotlight Icon -->
        <icon src="res/ios/appicon/icon-40.png" width="40" height="40" />
        <icon src="res/ios/appicon/icon-40@2x.png" width="80" height="80" />
        <!-- iPhone / iPod Touch -->
        <icon src="res/ios/appicon/icon.png" width="57" height="57" />
        <icon src="res/ios/appicon/icon@2x.png" width="114" height="114" />
        <!-- iPad -->
        <icon src="res/ios/appicon/icon-72.png" width="72" height="72" />
        <icon src="res/ios/appicon/icon-72@2x.png" width="144" height="144" />
        <!-- iPad Pro App iOS 9 83.5pt@2x -->
        <icon src="res/ios/appicon/icon-83.5@2x.png" width="167" height="167" />

        <!-- iPhone Spotlight and Settings Icon -->
        <icon src="res/ios/appicon/icon-small.png" width="29" height="29" />
        <icon src="res/ios/appicon/icon-small@2x.png" width="58" height="58" />
        <icon src="res/ios/appicon/icon-small@3x.png" width="87" height="87" />
        <!-- iPad Spotlight and Settings Icon -->
        <icon src="res/ios/appicon/icon-50.png" width="50" height="50" />
        <icon src="res/ios/appicon/icon-50@2x.png" width="100" height="100" />
        
        <!-- images are determined by width and height. The following are supported -->
        <splash src="res/ios/launchimage/Default~iphone.png" width="320" height="480"/>
        <splash src="res/ios/launchimage/Default@2x~iphone.png" width="640" height="960"/>
        <splash src="res/ios/launchimage/Default-Portrait~ipad.png" width="768" height="1024"/>
        <splash src="res/ios/launchimage/Default-Portrait@2x~ipad.png" width="1536" height="2048"/>
        <splash src="res/ios/launchimage/Default-Landscape~ipad.png" width="1024" height="768"/>
        <splash src="res/ios/launchimage/Default-Landscape@2x~ipad.png" width="2048" height="1536"/>
        <splash src="res/ios/launchimage/Default-568h@2x~iphone.png" width="640" height="1136"/>
        <splash src="res/ios/launchimage/Default-667h.png" width="750" height="1334"/>
        <splash src="res/ios/launchimage/Default-736h.png" width="1242" height="2208"/>
        <splash src="res/ios/launchimage/Default-Landscape-736h.png" width="2208" height="1242"/>

    </platform>
    
    <!--    设置启动页面停留时间-->
    <preference name="SplashScreenDelay" value="3000" />
    <!--  设置是否自动隐藏启动页面-->
    <preference name="AutoHideSplashScreen" value="true" />
    <!--    设置淡入淡出-->
    <preference name="FadeSplashScreen" value="true"/>
    <!--    是否显示风火轮-->
    <preference name="ShowSplashScreenSpinner" value="true"/>
    

</widget>
