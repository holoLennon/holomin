<!--Suggestion，意见反馈-->
<ion-view view-title="意见反馈"  cache-view="false">
	<ion-nav-buttons side="right">
		<h4></h4>
	</ion-nav-buttons>
	<ion-content class="has-tabs has-header">
		<div class="list">
			<div class="list list-inset">
        <label class="item item-input item-input-wrapper" style="height: 150px;overflow: visible;"><textarea row="3" style="height:100%;width: 100%;border:0.5px #ddd solid;padding: 5px;overflow: visible;background-color: #fff !important;" placeholder="您的意见建议是易健进步的动力"  ng-model="obj.detail"></textarea></label>
      </div>
			<a ng-click="vm.isedit=false;save()" class="button button-positive"style="margin-top:5px;margin-left: 20%;width:60%;background-color: #008cad" >提交意见</a>
		</div>
	</ion-content>
</ion-view>

