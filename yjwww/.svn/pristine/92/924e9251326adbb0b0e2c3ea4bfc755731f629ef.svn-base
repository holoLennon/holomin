<!--DoctorList，医生会员列表-->
<ion-view view-title="设置" >
    <ion-nav-buttons side="right">
        <h4></h4>
    </ion-nav-buttons>
    <ion-content class="has-tabs has-header">
        <div class="list" style="border: none;">
            <div class="item item-divider"  style="border: none;"></div>
            <a class="item" style="border: none" href="tel:\\0571-85389727">
                <p>
                    <span>电话客服：</span>
                    <span style="margin-top: -20px" >0571-85389727</span></br>
                </p>
            </a>
            <a class="item"style="border: none" href="sms:13346189099">
                <p>
                    <span>短信客服：</span>
                    <span style="margin-top: -20px" >13346189099</span>
                </p>
            </a>
            <a class="item"style="border: none" ng-click="editpassword()">
                <p>
                    <span>忘记密码：</span>
                    <span style="margin-top: -20px">找回密码</span>
                </p>
            </a>
            <div class="item item-divider"style="border: none"></div>
            <a class="item"style="border: none" ng-click="clear()">
                <p>
                    <span>清除系统缓存</span>
                </p>
            </a>
            <div class="item item-divider"style="border: none"></div>
            <a class="item"style="border: none" ng-click="rx('#/tab/yijian')">
                <p>
                  <span>关于我们[1.2.1(19)]</span>
                </p>
            </a>
            <div class="item item-divider" style="border: none"></div>
            <a  class="button button-positive"style="margin-top:5px;margin-left: 20%;width:60%;background-color: #008cad;text-align: center;" ng-click="logout()">退出当前用户</a>
        </div>
    </ion-content>
</ion-view>
