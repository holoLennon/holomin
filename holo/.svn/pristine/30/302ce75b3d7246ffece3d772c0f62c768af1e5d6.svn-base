<!--ProductTypeLinkProductList，商品类型包含商品列表-->
<ion-view view-title="商品类型包含商品列表"  >
	<ion-nav-buttons side="right">
		<h4></h4><a class="button icon ion-plus-round" href="#/tab/ProductTypeLinkProduct/0"></a>
	</ion-nav-buttons>
	<ion-content class="has-tabs has-header">
        <ion-refresher pulling-text="下拉刷新..." on-refresh="doRefresh()"></ion-refresher>
        <ion-list class="list">
            <ion-item class="item-avatar" bindonce collection-repeat="obj in list" href="#/tab/ProductTypeLinkProduct/{{obj.id}}" collection-item-width="'100%'" collection-item-height="65px">
                <img ng-src="{{obj.img1}}" reset-img />
                <h2>{{obj.myname}}</h2>
                <p>
				<span>序号:{{obj.id}}</span>
				<span>商品ID:{{obj.productIdString}}</span>
				<span>商品类型ID:{{obj.productTypeIdString}}</span>
				<span>排列顺序:{{obj.priority}}</span>
				<span>状态:{{obj.statusValidOrNotString}}</span>
				</p>
            </ion-item>
        </ion-list>
        <ion-infinite-scroll on-infinite="more()" distance="1%" ng-if="page.hasNextPage"></ion-infinite-scroll>
        <ion-spinner class="topic-spinner ng-hide" ng-show="!list" ng-style="{'top':getHalfHeight()+'px'}"></ion-spinner>
    </ion-content>
</ion-view>
