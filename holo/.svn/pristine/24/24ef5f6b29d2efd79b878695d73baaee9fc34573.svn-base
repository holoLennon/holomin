	/**我的中文名称*/
	private String myname;
	/**我的中文名称*/
	@Transient
	
	//@JSONField(serialize = false)
	public String getMyname() {
		myname=(myname==null)?""+nickname+" ":myname;
		return myname;
	}
	/**我的中文名称*/
	public void setMyname(String myname) {
