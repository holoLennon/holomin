	/**
	 * 返回所有项目的树形列表，注意这个会读全表，如果数据量大不要使用
	 * @param addprent boolean  是否加父，在json中，加了父就会死循环
	 * @return
	 */
	public List<ProductType> listWithTree(boolean addprent) throws BoException,Exception{
		QueryHelper queryHelper=new QueryHelper(); 
		queryHelper.setOrder("priority");
		commonzSvrUitlsSpec.makeWhereForListWithTree(null, "ProductType", queryHelper);
		List<ProductType> listAll=listfind(null, null, queryHelper.getWhere(), queryHelper.getOrder(), null);
		gridAfter(null, listAll);
		List<ProductType> listTree=new ArrayList<ProductType>();
		//找到顶级
		for (ProductType obj : listAll) {
			if(StringUtilz.integerNullOr0(obj.getParentid())){
				listTree.add(obj);
			}
		}
		//给所有对象加孩子列表
		for (ProductType obj : listTree) {
			putChildren(obj, listAll,addprent);
		}
		return listTree;
	}
