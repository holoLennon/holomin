<!--DoctorList，医生会员列表-->
<ion-view view-title="预约药品"  cache-view="false" hide-tabs="false">
	<ion-nav-buttons side="right">
		<h4></h4>
	</ion-nav-buttons>
	<ion-content class="has-tabs has-header">
		<div class="list">

			<div class="list list-inset">
				<label class="item item-input" style="height: 150px">
					<input style="margin-top: 0" type="text" placeholder="请预约您所需要的药品" ng-model="obj.detail">
				</label>

			</div>

			<a ng-click="vm.isedit=false;save()" class="button button-positive"style="margin-top:5px;margin-left: 20%;width:60%;background-color: #008cad" >提交预约</a>

			<!--<li > <a ng-click="vm.isedit=false;save()" class="button button-positive" style="left:20px"><i class="icon ion-checkmark"></i>提交意见</a>-->

			</li>
		</div>
	</ion-content>
</ion-view>

