<!--OrderrdetailList，订单详细列表-->
<ion-view view-title="订单详细列表" cache-view='false' hide-tabs="false">
	<ion-nav-buttons side="right">
		<h4></h4><a class="button icon ion-plus-round" href="#/tab/Orderrdetail/0"></a>
	</ion-nav-buttons>
	<ion-content class="has-tabs has-header">
        <ion-refresher pulling-text="下拉刷新..." on-refresh="doRefresh()"></ion-refresher>
        <ion-list class="list">
            <ion-item class="item-avatar" bindonce collection-repeat="obj in list" href="#/tab/Orderrdetail/{{obj.id}}" collection-item-width="'100%'" collection-item-height="65px">
                <img ng-src="{{obj.img1}}" reset-img />
                <h2>{{obj.myname}}</h2>
                <p>
				<span>序号ID:{{obj.id}}</span>
				<span class="item-note ago" am-time-ago="obj.gmtCreate"></span>
				<span class="item-note ago" am-time-ago="obj.gmtSent"></span>
				<span class="item-note ago" am-time-ago="obj.gmtRecv"></span>
				<span>订单号:{{obj.orderrIdString}}</span>
				<span>开单类型:{{obj.iotypeString}}</span>
				<span>支付状态:{{obj.statusString}}</span>
				<span>收发货状态:{{obj.statusgoodsString}}</span>
				<span>统计状态:{{obj.statuscountString}}</span>
				<span>会员:{{obj.memberIdString}}</span>
				<span>推荐医生:{{obj.doctorIdString}}</span>
				<span>产品:{{obj.productIdString}}</span>
				<span>商品名:{{obj.title}}</span>
				<span>厂商产地:{{obj.manufacturerIdString}}</span>
				<span>规格型号:{{obj.productstyle}}</span>
				<span>图1:{{obj.img1}}</span>
				<span>单价:{{obj.price}}</span>
				<span>数量:{{obj.num}}</span>
				<span>总价:{{obj.sumprice}}</span>
				<span>用药方法:{{obj.usemethord}}</span>
				<span>医嘱:{{obj.otherdetail}}</span>
				<span>用法用量:{{obj.usemount}}</span>
				</p>
            </ion-item>
        </ion-list>
        <ion-infinite-scroll on-infinite="more()" distance="1%" ng-if="page.hasNextPage"></ion-infinite-scroll>
        <ion-spinner class="topic-spinner ng-hide" ng-show="!list" ng-style="{'top':getHalfHeight()+'px'}"></ion-spinner>
    </ion-content>
</ion-view>
