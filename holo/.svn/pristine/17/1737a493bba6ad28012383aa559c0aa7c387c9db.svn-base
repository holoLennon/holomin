<ion-view view-title="注册" hide-back-button="true">

    <ion-content>
        <div class="list listreg">
            <form id="form">
                <input type="hidden" name="username" value="{{user.username}}">
                <input type="hidden" name="password" value="{{user.password}}">
                <input type="hidden" name="mobile" value="{{user.username}}">
                <div class="item item-icon-left">
                    <i class="icon ion-ios-telephone-outline"></i>
                    {{user.username}}
                </div>
                <label class="item-input item-floating-label">
                    <span class="input-label" style="padding-left: 15px;">别名或昵称</span>
                    <input id="realname" name="realname" type="text" placeholder="真实姓名" ng-model="member.realname" style="border-bottom:solid 1px #ccc;">
                </label>
                <a class="button button-block button-calm" ng-click="regcbCommit()" style="margin-top:30px;">
                    提交
                </a>
            </form>
        </div>
    </ion-content>
</ion-view>
